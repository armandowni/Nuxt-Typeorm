    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.8.2",
    "typeorm": "^0.3.7",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
